        "custom_liquid_k4xzTA": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "<div class=\"product__block product__block--custom_liquid\">\n            <style>\n  #people-viewing {\n    position: relative;\n    padding-left: 30px; /* Add padding to make space for the glow */\n    font-size: 14px;\n    font-weight: bold;\n  }\n\n  #people-viewing::before {\n    content: '';\n    position: absolute;\n    left: 0;\n    top: 50%;\n    transform: translateY(-50%);\n    width: 12px;\n    height: 12px;\n    border-radius: 50%;\n    background-color: #f4af29;\n    box-shadow: 0 0 10px #f4af29, 0 0 20px #f4af29;\n    animation: glowing 3.5s infinite; /* Apply animation */\n  }\n\n  /* Keyframes for glowing animation */\n  @keyframes glowing {\n    0% { box-shadow: 0 0 5px #f4af29, 0 0 10px #f4af29; }\n    50% { box-shadow: 0 0 20px #f4af29, 0 0 30px #f4af29; }\n    100% { box-shadow: 0 0 5px #f4af29, 0 0 10px #f4af29; }\n  }\n</style>\n\n<div id=\"people-viewing\">Plus que <span id=\"stock-count\">6</span> en stock.</div>\n\n<script>\n  // Function to change the stock number after 2 seconds\n  function changeStockNumber() {\n    setTimeout(() => {\n      document.getElementById(\"stock-count\").textContent = \"6\";\n    }, 2000);\n  }\n\n  // Using Intersection Observer API to detect when element is in viewport\n  const observer = new IntersectionObserver((entries, observer) => {\n    entries.forEach(entry => {\n      if (entry.isIntersecting) { \n        changeStockNumber(); // Trigger stock change when visible\n        observer.disconnect(); // Stop observing after first trigger\n      }\n    });\n  }, { threshold: 0.5 }); // Trigger when at least 50% of the element is visible\n\n  observer.observe(document.getElementById(\"people-viewing\"));\n</script>\n          </div>"
          }
        },
